{
  "kind": "collectionType",
  "collectionName": "table_orders",
  "info": {
    "singularName": "table-order",
    "pluralName": "table-orders",
    "displayName": "table-order"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "table": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::table.table"
    },
    "food_items": {
      "type": "component",
      "component": "booking.food-items",
      "repeatable": true
    },
    "token_status": {
      "type": "enumeration",
      "default": "Open",
      "enum": [
        "Open",
        "Closed"
      ]
    },
    "closing_method": {
      "type": "enumeration",
      "enum": [
        "Room Transfer",
        "Restaurant Invoice"
      ]
    },
    "restaurant_invoice": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::restaurant-invoice.restaurant-invoice",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "closing_method"
            },
            "Restaurant Invoice"
          ]
        }
      }
    },
    "room_no": {
      "type": "string",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "closing_method"
            },
            "Room Transfer"
          ]
        }
      }
    },
    "room_booking": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::room-booking.room-booking",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "closing_method"
            },
            "Room Transfer"
          ]
        }
      },
      "inversedBy": "table_orders"
    },
    "hotel_id": {
      "type": "string"
    },
    "order_id": {
      "type": "string"
    },
    "date": {
      "type": "date"
    },
    "time": {
      "type": "string"
    }
  }
}
